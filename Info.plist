<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Emerson Vinicius</string>
	<key>CFBundleDisplayName</key>
	<string>W3C Spec Styles</string>
	<key>CFBundleIdentifier</key>
	<string>com.duke.w3cspecstyles</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>Chrome</key>
	<dict/>
	<key>Content</key>
	<dict>
		<key>Blacklist</key>
		<array>
			<string>http://www.w3.org/</string>
			<string>http://www.w3.org/TR/</string>
			<string>http://w3.org/TR/</string>
			<string>http://w3.org/TR/tr-date-all*</string>
			<string>http://www.w3.org/TR/tr-date-all*</string>
		</array>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>javascripts/enhancements.js</string>
			</array>
		</dict>
	</dict>
	<key>Description</key>
	<string>An alternate stylesheet for W3C specification pages</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>*.w3.org</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Website</key>
	<string>https://github.com/benschwarz/w3c-spec-styles/</string>
</dict>
</plist>
